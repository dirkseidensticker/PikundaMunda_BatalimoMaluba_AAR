@article{Maley.2017,
  title = {Late {{Holocene}} Forest Contraction and Fragmentation in Central {{Africa}}},
  author = {Maley, Jean and Doumenge, Charles and Giresse, Pierre and Mah{\'e}, Gil and Philippon, Nathalie and Hubau, Wannes and Lokonda, Michel O. and Tshibamba, John M. and {Chepstow-Lusty}, Alex},
