  author = {Lupo, Karen D. and Schmitt, Dave N. and Ndanga, Jean-Paul and Nguerede, Lucien P. and Amaye, Guy T. and Smith, Alexandre Livingstone and Edwards, Nicolette M. and Power, Robert C. and Craig Young, D. and Npo, Frank},
