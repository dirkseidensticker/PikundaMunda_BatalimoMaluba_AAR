@article{Oslisly.2013b,
  title = {West {{Central African Peoples}}: {{Survey}} of {{Radiocarbon Dates}} over the {{Past}} 5000 {{Years}}},
  author = {Oslisly, Richard and Bentaleb, Ilham and Favier, Charly and Fontugne, Michel and Gillet, Jean Fran{\c c}ois and {Morin-Rivat}, Julie},
  year = {2013},
  journal = {Radiocarbon},
  volume = {55},
  number = {2--3},
  abstract = {Tracing human history in west central Africa suffers from a scarcity of historical data and archaeological remains. In order to provide new insight into this problem, we reviewed 733 radiocarbon dates of archaeological sites from the end of the Late Stone Age, Neolithic Stage, and Early and Late Iron Age in Cameroon, Gabon, Central African Republic, Equatorial Guinea, Republic of Congo, and the western Democratic Republic of Congo. This review provides a spatiotemporal framework of human settlement in the forest biome. Beyond the well-known initial spread of Iron Age populations through central African forests from 2500 cal BP, it depicts the geographical patterns and links with the cultural evolution of the successive phases of human expansion from 5000 to 3000 cal BP and then from 3000 to 1600 cal BP, of the hinterland depopulation from 1350 to 860 cal BP, and of recolonization up to 500 cal BP. DOI:\textasciitilde{} 10.2458/azu\_js\_rc.55.16385},
  langid = {english},
  keywords = {14C,Afrika/Zentral-,Besiedlungsgeschichte,MIA/Middle Iron Age},
  file = {C\:\\Users\\dirks\\OneDrive\\Literatur\\Oslisly et al_2013_West Central African Peoples\\Oslisly et al. - 2013 - West Central African Peoples Survey of Radiocarbo.csv;C\:\\Users\\dirks\\OneDrive\\Literatur\\Oslisly et al_2013_West Central African Peoples\\Oslisly et al. - 2013 - West Central African Peoples Survey of Radiocarbo.pdf;C\:\\Users\\dirks\\OneDrive\\Literatur\\Oslisly et al_2013_West Central African Peoples\\Oslisly et al. - 2013 - West Central African Peoples Survey of Radiocarbo.xlsx;C\:\\Users\\dirks\\OneDrive\\Literatur\\Oslisly et al_2013_West Central African Peoples\\Oslisly et al. - 2013 - West Central African Peoples Survey of Radiocarbo2.pdf;C\:\\Users\\dirks\\OneDrive\\Literatur\\Oslisly et al_2013_West Central African Peoples\\Oslisly et al. - 2013 - West Central African Peoples Survey of Radiocarbo2.xlsx}
